  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.7.25",
    "babel-runtime": "^6.20.0",
    "bluebird": "^3.4.7",
    "node-fetch": "^1.6.3",
    "serverless": "^1.5.1",
    "serverless-crypt": "^0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.13.1",
    "serverless-build-plugin": "^1.1.2",
    "serverless-mocha-plugin": "^1.3.2",
    "serverless-offline": "^3.8.3"
  },
  "scripts": {
    "clean": "basename `find src -depth 1` | xargs rm -rf .DS_Store .serverless",
    "prebuild": "yarn run lint",
    "build": "babel $BABEL_OPTS src -d .",
    "predeploy": "yarn run build",
    "deploy": "sls deploy $DEPLOY_OPTS",
    "deploy:dry": "DEPLOY_OPTS=--no-deploy yarn run deploy",
    "dev": "BABEL_OPTS=--watch yarn run build",
    "lint": "eslint src"
  }
